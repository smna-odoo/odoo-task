<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="ageing_selection_days" model="ir.ui.view">
        <field name="name">stock.quant.name</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"></field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='inventory_date']" position="after">
                <field name="ageing_selection"/>
            </xpath>
        </field>
    </record>

  
</odoo>
